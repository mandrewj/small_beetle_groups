    "markdown-it-anchor": "^8.6.7",
    "pinia": "^2.1.6",
    "unhead": "^1.7.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
